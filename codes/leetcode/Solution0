//reference: https://stackoverflow.com/questions/22352934/remove-duplicates-from-an-array-in-java
//Solution0

package leetcode;

import java.util.Arrays;
import java.util.HashSet;
import java.util.Set;

public class Solution0 {
	public int[] onlyUniqueElements(final int[] inputArray)
	{
	    final Set<Integer> set = new HashSet<>();
	    final int[] tmp = new int[inputArray.length];
	    int index = 0;
	    for (final int i: inputArray)
	        if (set.add(i))
	            tmp[index++] = i;

	    return Arrays.copyOfRange(tmp, 0, index);
	}
	
	public static void main(String[] args) {
		Solution0 obj = new Solution0();
		final int[] inputArray = {0,0,1,1,1,2,2,3,3,4};
		System.out.println(Arrays.toString(obj.onlyUniqueElements(inputArray)));
	}
	
}

	
